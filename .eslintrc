{
  "extends": ["react-app", "airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "prettier"],
  "rules": {
    "jsx-a11y/no-static-element-interactions": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": ["off"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  }
}
